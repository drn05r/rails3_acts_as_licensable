<% c_type = visible_name contributable %>

<div class="licensed_section_box">
	<p class="heading">
		License
		<a name="license"></a>
	</p>

    <% dc_title = licensable.title %>
    <% dc_description = h(licensable.respond_to?('description') ? licensable.description : licensable.body) %>
    <% dc_date = licensable.created_at %>
    <% dc_creator = "" %>
    <% dc_source = "" %>


    <%=  "<!--

    <rdf:RDF xmlns=\"http://creativecommons.org/ns#\"
        xmlns:dc=\"http://purl.org/dc/elements/1.1/\"
        xmlns:rdf=\"http://www.w3.org/1999/02/22-rdf-syntax-ns#\">
    <Work rdf:about=\"\">
       <dc:title>#{dc_title}</dc:title>
       <dc:date>#{dc_date}</dc:date>
       <dc:description>#{dc_description}</dc:description>
       <dc:creator><Agent>
          <dc:title>#{dc_creator}</dc:title>
       </Agent></dc:creator>
       <dc:rights><Agent>
          <dc:title>#{dc_creator}</dc:title>
       </Agent></dc:rights>
       <dc:type rdf:resource=\"http://purl.org/dc/dcmitype/Data\" />
       <dc:source rdf:resource=\"#{dc_source}\"/>
       <license rdf:resource=\"#{licensable.license.url}\" />
    </Work>

    <License rdf:about=\"#{licensable.license.url}\">"%>
    <% licensable.license.license_attributes.each do |attrib| %><%= "  <#{attrib.predicate} rdf:resource=\"#{attrib.uri}\" />" %><% end %>
    </License>

    </rdf:RDF>

    -->

  <% end %>

  <div class="licensable_currentlicense">
    <p>
      <%= licensable.title -%> is licensed under: <br/><br/><%= license_icon_link(@license) %>
    </p>
  </div>
	
</div>
